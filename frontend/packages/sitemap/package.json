{
  "name": "@ppt/sitemap",
  "private": true,
  "description": "Comprehensive sitemap for navigation paths, API endpoints, and user flows",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./json": {
      "types": "./src/json/sitemap.json",
      "import": "./src/json/sitemap.json"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "generate-json": "tsx src/json/generate.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^2.1.8"
  },
  "version": "0.2.278"
}
